{
  "name": "cinehall-client",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:web": "cd apps/web && yarn dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "clean": "rm -rf node_modules && rm -rf apps/*/node_modules && rm -rf packages/*/node_modules"
  },
  "repository": "git@github.com:d33pto3/cinehall-client.git",
  "author": "d33pto3 <rakinfrhn@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.11",
    "tailwindcss": "^4.0.11",
    "turbo": "^2.4.4"
  },
  "dependencies": {
    "expo-router": "^5.1.3",
    "expo-splash-screen": "^0.30.10"
  },
  "resolutions": {
    "react": "19.1.0"
  }
}
